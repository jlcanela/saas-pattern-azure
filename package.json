    "effect": "3.12.5"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "turbo": "latest"
  },
  "packageManager": "pnpm@10.11.0",
  "name": "with-vite",
  "pnpm": {
    "overrides": {
      "vite@>=5.0.0 <=5.4.18": ">=5.4.19",
      "brace-expansion@>=2.0.1 <=4.0.0": ">=4.0.1"
    }
  }
}
